if test "x$ac_cv_header_audio_audiolib_h" = xyes; then :
  have_nas_hdr=yes
fi


        { $as_echo "$as_me:${as_lineno-$LINENO}: checking for AuOpenServer in -laudio" >&5
$as_echo_n "checking for AuOpenServer in -laudio... " >&6; }
